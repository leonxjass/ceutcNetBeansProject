package proytiempo;

/**
 *
 * @author tguca-L01
 */
public class repaso {
    public static int sumarhasta(int a[],int b){
        int sumador = 0;
        
        for(int i=0; i<=b; i++){
            sumador+=a[i];
        }
        
        return sumador;
    }
    
    public static void main(String[] arg){
        int[] a = {82,19,74,27,45,22,85};
        
        
        
        System.out.println(sumarhasta(a,4)-sumarhasta(a,1));
    }    
}
